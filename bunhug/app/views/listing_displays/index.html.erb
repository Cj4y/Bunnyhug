<p id="notice"><%= notice %></p>

<h1>Listing Displays</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listing_displays.each do |listing_display| %>
      <tr>
        <td><%= link_to 'Show', listing_display %></td>
        <td><%= link_to 'Edit', edit_listing_display_path(listing_display) %></td>
        <td><%= link_to 'Destroy', listing_display, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing Display', new_listing_display_path %>
