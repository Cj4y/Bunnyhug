<p id="notice"><%= notice %></p>

<h1>Star Ratings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @star_ratings.each do |star_rating| %>
      <tr>
        <td><%= link_to 'Show', star_rating %></td>
        <td><%= link_to 'Edit', edit_star_rating_path(star_rating) %></td>
        <td><%= link_to 'Destroy', star_rating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Star Rating', new_star_rating_path %>
